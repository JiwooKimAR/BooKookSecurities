            <TableView prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
              </columns>
            </TableView>
            <HBox>
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <Label id="label_itemSelected" text="0개가 선택됨." />
               </children>
            </HBox>
            <HBox spacing="10.0">
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <Button mnemonicParsing="false" prefWidth="100.0" text="닫기" />
                  <Button mnemonicParsing="false" prefWidth="100.0" text="삭제" />
